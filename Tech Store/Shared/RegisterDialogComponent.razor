<MudDialog Style="width:90%">
    <DialogContent>
        
        <RegisterComponent />

    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>

@code{
    [CascadingParameter] MudDialogInstance? MudDialog { get; set; }
    private void Submit()
    {
        MudDialog.Close(DialogResult.Ok(true));
    }
}